<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.caojun.salmagundi">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.NFC"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>

    <application
        android:name=".MainApplication"
        android:allowBackup="true"
        android:anyDensity="false"
        android:icon="@drawable/ic_launcher"
        android:label="@string/main_app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppStartLoadTranslucent">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".qrcode.QRCodeActivity"
            android:label="@string/qrcode_title" />
        <activity android:name=".color.ColorActivity"
            android:label="@string/color_title" />
        <activity android:name=".bankcard.BankCardActivity"
            android:label="@string/bankcard_title" />
        <activity android:name=".string.StringActivity"
            android:label="@string/string_title" />
        <activity android:name=".secure.SecureActivity"
            android:label="@string/secure_title" />
        <activity android:name=".rxjava.RxJavaActivity"
            android:label="@string/rxjava_title" />
        <activity android:name=".ai.AIActivity"
            android:label="@string/ai_title" />
        <activity android:name=".textart.TextArtActivity"
            android:label="@string/textart_title" />
        <activity android:name=".passwordstore.PasswordStoreActivity"
            android:label="@string/passwordstore_title" />
        <activity android:name=".passwordstore.PasswordDetailActivity"
            android:label="@string/ps_detail_title" />
        <activity android:name=".lockpattern.GestureLoginActivity"
            android:label="@string/gesture_login_title" />
        <activity android:name=".lockpattern.CreateGestureActivity"
            android:label="@string/create_gesture_title" />
    </application>

</manifest>